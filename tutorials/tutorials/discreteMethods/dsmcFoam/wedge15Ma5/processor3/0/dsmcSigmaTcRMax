boundaryField
{
    flow
    {
        type            zeroGradient;
    }
    obstacle
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 2.30575378384293e-16;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 2.30575378384293e-16;
    }
