FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.007";
    object      boundaryT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    flow
    {
        type            fixedValue;
        value           uniform 300;
    }
    obstacle
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0;
    }
}


