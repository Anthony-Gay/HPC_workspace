FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    sides
    {
        type            codedFixedValue;
        value           $internalField;
        redirectType    HPCFluid;
        code
  #{
        operator==(min(10, 0.1));
  #};

    //type            fixedValue;
    //value           uniform 1;
        
    }

    empty
    {
        type            empty;
    }
}

