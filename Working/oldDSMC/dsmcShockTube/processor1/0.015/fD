FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.015";
    object      fD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    sides
    {
        type            symmetry;
    }
    empty
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


