FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


defaultFieldValues
(
    volVectorFieldValue U (0 0 0)
    volScalarFieldValue T 0
    volScalarFieldValue p 0
);

regions
(
    zoneToCell
    {
        name "fluid";
            fieldValues
            (
                volVectorFieldValue U (2 0 0)
                volScalarFieldValue T 348.432
                volScalarFieldValue p 100000
            );
    }
);
