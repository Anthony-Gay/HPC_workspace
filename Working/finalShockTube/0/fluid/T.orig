FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 900;//0.0034;

boundaryField
{
    fluidBound
    {
        //type            zeroGradient; 
        type            codedFixedValue;
        value           $internalField;
        redirectType    HPCFluidT;
    }

    fluidSide
    {
        type            zeroGradient; 
    }

    nullDims
    {
        type            empty;
    }
}

