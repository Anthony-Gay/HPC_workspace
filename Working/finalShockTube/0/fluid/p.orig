FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    fluidBound
    {
        type            codedFixedValue;
        value           $internalField;
        redirectType    HPCFluid;
    }
    
    fluidSide
    {
        type            zeroGradient;
    }
    /*fluidBound
    {
        type               oneDirectionalHybridFluid; 
        fluidIndex         99;
        particleIndex      0;
        w                  0.75;
        value              0;
    }*/

    

    nullDims
    {
        type            empty;
    }
}
