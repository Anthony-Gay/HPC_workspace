FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1.1288316e2; //1.242562104e2;

boundaryField
{
    fluidBound
    {
       //type     zeroGradient;
        type            codedFixedValue;
        value           $internalField;
        redirectType    HPCFluidP;
    }
    
    fluidSide
    {
        type            zeroGradient;
    }

    nullDims
    {
        type            empty;
    }
}
