FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 348.432;

boundaryField
{
    fluidSide
    {
        type            zeroGradient;
    }
    fluidBound
    {
        type            zeroGradient;
    }
    nullDims
    {
        type            empty;
    }
}


