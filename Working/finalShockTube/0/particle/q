FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      q;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    DSMCBound
    {
        type            symmetry;
    }

    DSMCSide
    {
        type            symmetry;
    }

    nullDims
    {
        type            empty;
    }
}

