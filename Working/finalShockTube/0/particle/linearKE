FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      linearKE;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    partBound
    {
        type            zeroGradient;//symmetry;//zeroGradient;
    }

    partSide
    {
        type            symmetry;//symmetry;//symmetry;//symmetry;
               

    }

    nullDims
    {
        type            empty;
    }
}


