FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      boundaryT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 900;

boundaryField
{
    
    DSMCBound
    {
      // type symmetry;
        type            codedFixedValue;
        value           $internalField;
        redirectType    HPCParticleT;
    }

    DSMCSide
    {
        type            symmetry;
    }

    nullDims
    {
        type            empty;
    }
}


